    function instantiate_area(id, object_type, url, instances) {
        var obj = null;
        if (instances[id]) {
            obj = instances[id];
        } else {
            obj = new object_type();
            instances[id] = obj;
        };

        obj.deferred = obj.fetch({
            url : url,
            async: true,
        });
        
        return obj;
    };

