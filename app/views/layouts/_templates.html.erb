<script type="text/template" id="schedule-conflict-view-template">
    <div class="thumbnail clearfix">
        <button type="button" class="close exclude" aria-hidden="true">×</button>
        <small class='conflict'>
            <b>{{= person_name }}</b> <%= t "has-a-conflict" %> <br/>
            {{= room_name}}: <b>{{= item_title }}</b> ({{= item_role}}) <%= t "at" %> {{= item_day_string }} {{= item_start_string }} <br/>
            <%=t "with" %> {{= conflict_room_name}}: <b>{{= conflict_item_title }}</b> ({{= conflict_item_role}}) <%= t "at" %> {{= conflict_day_string }} {{= conflict_start_string }}
        </small>
    </div>
</script>
